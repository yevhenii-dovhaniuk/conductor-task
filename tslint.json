{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-console": false,
    "ordered-imports": false,
    "interface-name": false,
    "member-ordering": false,
    "no-debugger": false,
    "jsx-boolean-value": false,
    "quotemark": [true, "single", "jsx-double"]
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
